{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@angular-material-extensions/dialogs": [
        "dist/angular-material-extensions/dialogs"
      ],
      "@angular-material-extensions/dialogs/*": [
        "dist/angular-material-extensions/dialogs/*"
      ],
      "@angular-material-extensions/core": [
        "dist/angular-material-extensions/core"
      ],
      "@angular-material-extensions/core/*": [
        "dist/angular-material-extensions/core/*"
      ],
      "@angular-material-extensions/pages": [
        "dist/angular-material-extensions/pages"
      ],
      "@angular-material-extensions/pages/*": [
        "dist/angular-material-extensions/pages/*"
      ]
    }
  }
}