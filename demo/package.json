{
  "name": "@angular-material-extensions/pages-demo",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Demo app for @angular-material-extensions/pages",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:prod": "ng serve --prod",
    "build": "ng build",
    "build:doc": "cd ../ && gulp build:doc",
    "build:prod": "ng build --prod ",
    "build:client-and-server-bundles": "ng build --prod &&  ng run ngx-auth-firebaseui:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "deploy:firebase": "npm run build:prod && npm run build:doc && firebase deploy",
    "e2e": "ng e2e",
    "generate:prerender": "cd dist && node prerender",
    "lint": "ng lint",
    "test": "ng test",
    "postinstall": "npm run link",
    "link": "npm link @angular-material-extensions/pages",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.11",
    "@angular/cdk": "^7.3.6",
    "@angular/common": "^7.2.11",
    "@angular/compiler": "^7.2.11",
    "@angular/core": "^7.2.11",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.2.11",
    "@angular/http": "^7.2.11",
    "@angular/material": "^7.3.6",
    "@angular/platform-browser": "^7.2.11",
    "@angular/platform-browser-dynamic": "^7.2.11",
    "@angular/platform-server": "^7.2.11",
    "@angular/router": "^7.2.11",
    "@ng-bootstrap/ng-bootstrap": "^4.1.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.1",
    "font-awesome": "^4.7.0",
    "include-media": "^1.4.9",
    "material-design-icons": "^3.0.1",
    "ngx-clipboard": "^11.1.9",
    "ngx-disqus": "^2.4.2",
    "ngx-highlightjs": "^2.1.4",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.7",
