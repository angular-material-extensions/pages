{
  "name": "@angular-material-extensions/components",
  "description": "Set of components, directives, services and modules to boost the app development with angular material ",
  "version": "0.0.0",
  "homepage": "https://github.com/angular-material-extensions/components/tree/master/projects/angular-material-extensions/dialogs",
  "author": {
    "name": "anthonynahas",
    "url": "https://github.com/anthonynahas"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular-material-extensions/components.git"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod && npm run link:all",
    "build:client-and-server-bundles": "npm run build:prod && ng run components:server:production",
    "build:dialogs": "ng build @angular-material-extensions/dialogs",
    "build:dialogs:watch": "ng build @angular-material-extensions/dialogs --watch",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "deploy:firebase": "firebase deploy",
    "e2e": "ng e2e",
    "generate:prerender": "cd dist && node prerender",
    "lint": "ng lint",
    "link:dialogs": "cd dist/angular-material-extensions/dialogs && npm link",
    "link:libraries": "npm link @angular-material-extensions/dialogs",
    "link:all": "npm run link:dialogs && npm run link:libraries",
    "test": "ng test",
    "semantic-release": "semantic-release",
    "semantic-release-no-ci": "semantic-release --no-ci",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.1",
    "@angular/cdk": "^8.0.2",
    "@angular/common": "^8.1.1",
    "@angular/compiler": "^8.1.1",
    "@angular/core": "^8.1.1",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.1.1",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "^8.1.1",
    "@angular/platform-browser-dynamic": "^8.1.1",
    "@angular/platform-server": "^8.1.1",
    "@angular/router": "^8.1.1",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "ngx-markdown": "^8.1.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "ws": "^7.1.0",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.801.1",
    "@angular-devkit/build-ng-packagr": "^0.801.1",
    "@angular/cli": "^8.1.1",
    "@angular/compiler-cli": "^8.1.1",
    "@angular/language-service": "^8.1.1",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.6.2",
    "codelyzer": "^5.0.0",
    "firebase-tools": "^7.1.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.0.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "schematics-utilities": "latest",
    "semantic-release": "^15.13.18",
    "ts-loader": "^6.0.4",
    "ts-node": "~8.3.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.4.5",
    "webpack-cli": "^3.3.6"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist/**/*.{js,css}",
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "preset": "angular"
  }
}
